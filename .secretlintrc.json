{
    "rules": [
        {
            "id": "@secretlint/aws",
            "options": {
              "patterns": 
               [
                {
                  "name": "token",
                  "pattern": "/AWS-TOKEN\\s*=\\s*(?<AWS-TOKEN>[\\w\\d!@#$%^&(){}\\[\\]:\";'<>,.?\/~`_+-=|]{1,256})/"
                }
               ]
            }          
        }
    ]
}
